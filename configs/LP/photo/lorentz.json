{
    "task": "LP",
    "dataset": "photo",
    "root_path": "./datasets",
    "eval_freq": 10,
    "exp_iters": 5,
    "save_embeds": "./results//embeds.npy",
    "log_path": "./results//LP_lorentz_photo.log",
    "self_train": false,
    "epochs": 50,
    "lr": 0.01,
    "w_decay": 0.0,
    "T": 5,
    "manifold": "lorentz",
    "backbone": "gcn",
    "n_layers": 1,
    "n_heads": 8,
    "embed_dim": 32,
    "step_size": 0.1,
    "v_threshold": 1.0,
    "dropout": 0.0,
    "epochs_cls": 200,
    "lr_cls": 0.01,
    "w_decay_cls": 5e-05,
    "patience_cls": 3,
    "save_path_cls": "./checkpoints/cls.pth",
    "lr_lp": 0.001,
    "w_decay_lp": 0,
    "epochs_lp": 200,
    "patience_lp": 3,
    "save_path_lp": "./checkpoints/lp.pth",
    "t": 1.0,
    "r": 2.0,
    "temperature": 0.2,
    "use_gpu": true,
    "gpu": 0,
    "devices": "0,1"
}